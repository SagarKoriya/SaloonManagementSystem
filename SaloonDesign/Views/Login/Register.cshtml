<body>
    <div class="container-scroller" >
        <div class="container-fluid page-body-wrapper full-page-wrapper">
            <div class="main-panel bgimg" style=" background-image: url('https://res.cloudinary.com/dzer9awba/image/upload/v1678199872/.net/STYLETTE_todp2v.png'); background-position: center; background-size: cover; background-repeat: no-repeat;">
                <div class="content-wrapper d-flex align-items-center auth px-0">
                    <div class="row w-100 mx-0 ">
                        <div class="col-lg-5 mx-auto">
                            <div class="shadow box-shadow border border-2 border-secondary bg-light bg-opacity-75 text-left py-5 px-4 px-sm-5 rounded-3 ">
     
                                <form class="">
                                    <div class="form-group">
                                        <label class="text-black" style="font-size: larger;">Name</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent ">
                                                <span class="input-group-text bg-transparent border border-1 border-dark rounded-2">
                                                    <i class="mdi mdi-account-outline text-primary"></i>
                                                </span>
                                            </div>
                                            <input type="text" class="form-control form-control-lg border-left-0 border border-1 border-dark rounded-2" placeholder="Name">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="text-black" style="font-size: larger;">Username</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent">
                                                <span class="input-group-text bg-transparent border border-1 border-dark rounded-2">
                                                    <i class="mdi mdi-account-box-outline text-primary"></i>
                                                </span>
                                            </div>
                                            <input type="text" class="form-control form-control-lg border-left-0 border border-1 border-dark rounded-2" placeholder="Username">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="text-black" style="font-size: larger;">Email</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent">
                                                <span class="input-group-text bg-transparent border border-1 border-dark rounded-2">
                                                    <i class="mdi mdi-email-outline text-primary"></i>
                                                </span>
                                            </div>
                                            <input type="email" class="form-control form-control-lg border-left-0 border border-1 border-dark rounded-2" placeholder="Email">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="text-black" style="font-size: larger;">Mobile No.</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent">
                                                <span class="input-group-text bg-transparent border border-1 border-dark rounded-2">
                                                    <i class="mdi mdi-cellphone text-primary"></i>
                                                </span>
                                            </div>
                                            <input type="text" class="form-control form-control-lg border-left-0 border border-1 border-dark rounded-2" placeholder="Mobile No.">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="text-black" style="font-size: larger;">Date of birth</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent">
                                                <span class="input-group-text bg-transparent border border-1 border-dark rounded-2">
                                                    <i class="mdi mdi-calendar text-primary"></i>
                                                </span>
                                            </div>
                                            <input type="date" class="form-control form-control-lg border-left-0 border border-1 border-dark rounded-2" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="text-black" style="font-size: larger;">Password</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent">
                                                <span class="input-group-text bg-transparent border border-1 border-dark rounded-2">
                                                    <i class="mdi mdi-lock-outline text-primary"></i>
                                                </span>
                                            </div>
                                            <input type="password" class="form-control form-control-lg border-left-0 border border-1 border-dark rounded-2" id="exampleInputPassword" placeholder="Password">
                                        </div>
                                    </div>

                                    <div class="mt-3 d-flex justify-content-center">
                                        <a class="btn btn-warning font-weight-bold vw-100 btn-md btn-fw" href="">SIGN UP</a>
                                    </div>
                                    <div class="text-center mt-4 font-weight-light text-black" style="font-size: medium;">
                                        Already have an account? <a asp-controller="Login" asp-action="Login" class="text-primary">Login</a>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- content-wrapper ends -->
        </div>
        <!-- page-body-wrapper ends -->
